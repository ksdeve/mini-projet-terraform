		  "url": { "raw": "http://172.190.162.177:8080/user/1", "host": ["http://172.190.162.177:8080"], "path": ["user", "1"] },
		  "body": { "mode": "raw", "raw": "{\"name\": \"John Updated\", \"email\": \"john.updated@example.com\"}" }
